    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "async": "^2.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "debug": "^2.2.0",
    "gray-matter": "^3.0.2",
    "hercule": "github:contentascode/hercule#resolver-placeholder",
    "json-pointer": "^0.6.0",
    "minimatch": "^3.0.0",
    "pegjs": "^0.10.0"
